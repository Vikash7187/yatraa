{
  "name": "yatraa",
  "private": true,
  "version": "1.0.0",
  "description": "Yatraa - Modern travel website for luxury hotels and destinations",
  "type": "module",
  "homepage": "https://Vikash7187.github.io/yatraa",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vikash7187/yatraa.git"
  },
  "keywords": [
    "travel",
    "hotel",
    "booking",
    "luxury",
    "tourism",
    "react",
    "vite",
    "mui"
  ],
  "author": "Yatraa Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "npx vite build",
    "preview": "vite preview",
    "start": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "mock-api": "node mock/server.mjs",
    "build:netlify": "npm install && npx vite build",
    "build:vercel": "npm install && npx vite build",
    "build:backend": "cd backend && npm install",
    "start:backend": "cd backend && npm start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.46.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.3",
    "@mui/material": "^7.3.3",
    "@mui/x-date-pickers": "^8.5.1",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.17.0",
    "jquery": "^3.7.1",
    "json-server": "^1.0.0-beta.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
